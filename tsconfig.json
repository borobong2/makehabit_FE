{
	"compilerOptions": {
		"jsx": "react",
		"esModuleInterop": true,
		"strict": false,
		"typeRoots": ["./@types", "./node_modules/@types"],
		"outDir": "./dist",
		"noImplicitAny": false,
		"module": "ES6",
		"target": "ES5",
		"allowJs": true,
		"checkJs": false,
		"sourceMap": true,
		"moduleResolution": "node"
	},
	"include": ["src", "src/custom.d.ts"]
}
